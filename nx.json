{
  "$schema": "./node_modules/nx/schemas/nx-schema.json",
  "defaultBase": "master",
  "namedInputs": {
    "default": [
      "{projectRoot}/**/*",
      "sharedGlobals"
    ],
    "production": [
      "default"
    ],
    "sharedGlobals": [
      "{workspaceRoot}/go.mod"
    ]
  },
  "plugins": [
    "@nx-go/nx-go"
  ],
  "pluginsConfig": {
    "@nx/go": {
      "analyzeSourceFiles": true
    }
  }
}
